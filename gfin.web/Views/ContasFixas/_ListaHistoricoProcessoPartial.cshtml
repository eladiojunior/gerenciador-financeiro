@model GFin.Web.Models.HistoricoProcessoModel
<div class="modal fade" id="modalHistoricoProcesso" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Histórico do Processo de Verificação de Contas Fixas</h4>
            </div>
            <div class="modal-body" style="height: 500px;overflow-y: auto;">
                <div class="row mensagens">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        <div class="alert"></div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
                        <div class="form-group">
                            <label class="control-label" for="DataHoraProcesso">Data/Hora</label>
                            @Html.TextBoxFor(model => model.DataHoraProcesso, new { @readonly = true, @class = "form-control text-center" })
                        </div>
                    </div>
                    <div class="col-xs-8 col-sm-8 col-md-8 col-lg-8">
                        <div class="form-group">
                            <label class="control-label" for="NomeProcesso">Descrição do Processo</label>
                            @Html.TextBoxFor(model => model.NomeProcesso, new { @readonly = true, @class = "form-control" })
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 col-xs-12">
                        <table class="table table-responsive">
                            <thead>
                                <tr>
                                    <th class="col-md-3 text-center">Data/Hora</th>
                                    <th class="col-md-2">Situação</th>
                                    <th class="col-md-7">Histórico</th>
                                </tr>
                            </thead>
                            <tbody>
                                @{
                                    if (Model.ListaHistoricoProcesso != null)
                                    {
                                        foreach (var item in Model.ListaHistoricoProcesso)
                                        {
                                            <tr>
                                                <td class="text-center">@item.DataHoraSituacaoProcesso.ToString("dd/MM/yyyy HH:mm:ss")</td>
                                                <td>@GFin.Dados.Enums.UtilEnum.GetTextoTipoSituacaoProcesso(item.CodigoTipoSituacaoProcesso)</td>
                                                <td>@item.TextoHistoricoSituacaoProcesso</td>
                                            </tr>
                                        }
                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-sm btn-default" data-dismiss="modal">Fechar</button>
            </div>
        </div>
    </div>
</div>