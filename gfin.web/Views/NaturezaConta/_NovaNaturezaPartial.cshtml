@model GFin.Web.Models.NovaNaturezaContaModel
<div class="modal fade" id="modalNaturezaConta" tabindex="-1" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Nova Natureza da Conta - @Model.TipoLancamentoConta</h4>
            </div>
            <form id="formNaturezaConta" onsubmit="return false;">
                @Html.HiddenFor(m => m.CodigoTipoLancamentoConta)
                @Html.HiddenFor(m => m.IdDropdownRetorno)
                <div class="modal-body">
                    <div class="row mensagens">
                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                            <div class="alert"></div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                            <div class="form-group">
                                <label for="DescricaoNaturezaConta">Descrição da Natureza:</label>
                                @Html.TextBoxFor(m=>m.DescricaoNaturezaConta, new { @maxlength = 100, @required = true, @class = "form-control", @placeholder = "Descrição da natureza da conta" })
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="submit" class="btn btn-primary">Registrar</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
                </div>
            </form>
        </div>
    </div>
</div>